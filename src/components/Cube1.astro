<svg viewBox="-100 -100 200 200" class="cube">
    <!-- Front face -->
    <rect x="-50" y="-50" width="100" height="100" fill="rgba(255, 0, 0, 0.9)" />
    <!-- Back face -->
    <rect x="-50" y="-50" width="100" height="100" fill="rgba(0, 255, 0, 0.9)" transform="translateZ(-100)" />
    <!-- Left face -->
    <rect x="-50" y="-50" width="100" height="100" fill="rgba(0, 0, 255, 0.9)" transform="rotateY(-90) translateZ(-50)" />
    <!-- Right face -->
    <rect x="-50" y="-50" width="100" height="100" fill="rgba(255, 255, 0, 0.9)" transform="rotateY(90) translateZ(-50)" />
    <!-- Top face -->
    <rect x="-50" y="-50" width="100" height="100" fill="rgba(255, 165, 0, 0.9)" transform="rotateX(90) translateZ(-50)" />
    <!-- Bottom face -->
    <rect x="-50" y="-50" width="100" height="100" fill="rgba(128, 128, 128, 0.9)" transform="rotateX(-90) translateZ(-50)" />
</svg>

<style>
    body {
        display: flex;
        justify-content: center;
        align-items: center;
        height: 100vh;
        background-color: #f4f4f4;
        margin: 0;
    }

    .cube {
        animation: rotate 5s infinite linear;
        transform-style: preserve-3d;
    }

    @keyframes rotate {
        from {
            transform: rotateX(0deg) rotateY(0deg);
        }
        to {
            transform: rotateX(360deg) rotateY(360deg);
        }
    }

    svg {
        width: 200px;
        height: 200px;
    }
</style>