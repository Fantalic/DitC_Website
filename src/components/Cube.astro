<div class="scene">
    <div class="cube">
        <div class="face front"></div>
        <div class="face back"></div>
        <div class="face left"></div>
        <div class="face right"></div>
        <div class="face top"></div>
        <div class="face bottom"></div>
    </div>
</div>
    <style>
        .scene {
            display: flex;
            justify-content: center;
            align-items: center;
            margin: 0;
            width: 100px;
            height: 100px;
            perspective: 600px;
        }

        .cube {
            width: 100%;
            height: 100%;
            position: relative;
            transform-style: preserve-3d;
            animation: rotate 10s infinite linear;
        }

        .face {
            position: absolute;
            width: 100px;
            height: 100px;
            background: rgba(255, 255, 255, 0.9);
            border: 1px solid #000;
        }

        .front { transform: translateZ(50px); background: rgba(80, 78, 78, 0.9); }
        .back { transform: rotateY(180deg) translateZ(50px); background: rgba(46, 48, 46, 0.9); }
        .left { transform: rotateY(-90deg) translateZ(50px); background: rgba(30, 30, 31, 0.9); }
        .right { transform: rotateY(90deg) translateZ(50px); background: rgba(27, 27, 26, 0.9); }
        .top { transform: rotateX(90deg) translateZ(50px); background: rgba(31, 30, 30, 0.9); }
        .bottom { transform: rotateX(-90deg) translateZ(50px); background: rgba(43, 41, 41, 0.9); }

        @keyframes rotate {
            from {
                transform: rotateX(0deg) rotateY(0deg);
            }
            to {
                transform: rotateX(360deg) rotateY(360deg);
            }
        }
    </style>

